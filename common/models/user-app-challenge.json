{
  "name": "UserAppChallenge",
  "base": "PersistedModel",
  "plural": "user-challenges",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": {}
  },
  "properties": {
    "isFinished": {
      "type": "boolean",
      "default": false
    },
    "expirationDate": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "challenge": {
      "type": "belongsTo",
      "model": "Challenge",
      "foreignKey": "challengeId"
    },
    "appUser": {
      "type": "belongsTo",
      "model": "AppUserAccount",
      "foreignKey": "userId"
    },
    "child": {
      "type": "belongsTo",
      "model": "Child",
      "foreignKey": "childId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "findById"
    }
  ],
  "methods": {}
}
