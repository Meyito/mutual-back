{
  "name": "Challenge",
  "plural": "challenges",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "minAge": {
      "type": "number",
      "required": true
    },
    "maxAge": {
      "type": "number",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "string",
      "required": true
    },
    "expPoints": {
      "type": "number",
      "default": 0
    },
    "frequency": {
      "type": "number"
    },
    "expireAt": {
      "type": "number",
      "required": true,
      "default": 7
    },
    "weight": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "reviewQuestions": {
      "type": "hasMany",
      "model": "ReviewQuestion",
      "foreignKey": "challengeId"
    }
  },
  "acls": [],
  "methods": {}
}
