{
  "name": "Child",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "birthday": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "characteristics": {
      "type": "hasMany",
      "model": "ChildCharacteristic",
      "foreignKey": "childId"
    },
    "gender": {
      "type": "hasOne",
      "model": "Gender",
      "foreignKey": "genderId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    }
  ],
  "methods": {}
}
