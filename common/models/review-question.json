{
  "name": "ReviewQuestion",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "content": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "challenge": {
      "type": "belongsTo",
      "model": "Challenge",
      "foreignKey": "challengeId"
    },
    "characteristic": {
      "type": "belongsTo",
      "model": "Characteristic",
      "foreignKey": "characteristicId"
    },
    "answers": {
      "type": "hasMany",
      "model": "ReviewAnswer",
      "foreignKey": "questionId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    }
  ],
  "methods": {}
}
