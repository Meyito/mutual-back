{
  "name": "mutual-back",
  "version": "1.0.0",
  "main": "dist/server/server.js",
  "scripts": {
    "start": "node .",
    "pretest": "gulp build && echo \"jshint . will disabled until ES7 is supported\"",
    "test": "mocha ./dist/**/*.spec.js",
    "cron:assignChallenges": "node scripts/triggerCron.js --cron=assignChallenges",
    "findc":"docker ps -a -q --filter \"ancestor=garusis/mutual\"",
    "stopc": "docker stop $(npm -s run findc)",
    "removec": "docker rm $(npm -s run stopc)",
    "removei": "docker rmi --force garusis/mutual",
    "buildc": "./launcher.sh & npm run removec 2>/dev/null & npm run removei 2>/dev/null & docker build -t garusis/mutual .",
    "ps": "docker ps -q --filter \"ancestor=garusis/mutual\"",
    "ps:run": "npm run ps -- --filter \"status=running\"",
    "ps:exit": "npm run ps -- --filter \"status=exited\"",
    "bash": "docker exec -i -t mutualapp bash",
    "bashdb": "docker exec -i -t mutual-postgres bash",
    "logs": "docker logs mutualapp",
    "runc": "docker run -d -p 3000:3000 --link \"mutual-postgres:pg\" --name mutualapp --restart=always --env-file ./env.list garusis/mutual:latest"
  },
  "dependencies": {
    "async": "^2.1.4",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "compression": "^1.0.3",
    "config": "^1.24.0",
    "cors": "^2.5.2",
    "debug": "^2.6.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^1.9.1",
    "jshint": "^2.5.6",
    "knex": "^0.12.6",
    "lodash": "^4.17.2",
    "loopback": "^2.22.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^2.1.0",
    "loopback-component-passport": "^2.3.0",
    "loopback-component-storage": "^3.0.0",
    "loopback-connector-mysql": "^2.4.0",
    "loopback-connector-postgresql": "^2.7.0",
    "loopback-datasource-juggler": "^2.39.0",
    "merge-stream": "^1.0.0",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "node-pushnotifications": "^1.0.14",
    "node-xlsx": "^0.7.4",
    "nodemailer": "^2.7.0",
    "pg": "^6.1.2",
    "pickup": "^5.1.0",
    "request": "^2.79.0",
    "run-sequence": "^1.2.2",
    "serve-favicon": "^2.0.1",
    "strong-cluster-control": "^2.2.3",
    "strong-globalize": "^2.8.0",
    "xlsx-template": "0.0.7",
    "xoauth2": "^1.2.0",
    "yargs": "^6.6.0"
  },
  "devDependencies": {},
  "repository": {
    "type": "",
    "url": ""
  },
  "engines": {
    "node": "4.6.2"
  },
  "description": "mutual-back"
}
